   "source": [
    "x = torch.tensor([1.0])\n",
    "y = torch.tensor([2.0])\n",
    "\n",
    "w = torch.tensor([1.0], requires_grad=True)\n",
    "\n",
    "## forward pass ##\n",
    "y_hat = w*x\n",
    "loss  = (y_hat - y)**2\n",
    "\n",
    "print(loss)\n",
    "\n",
    "## backward pass ##\n",
    "loss.backward()\n",
    "print(f\"w_grad = {w.grad}\")\n"
   ]
